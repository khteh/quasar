{
  "name": "khteh",
  "version": "1.0.0",
  "description": "khteh's app using Quasar Framework",
  "productName": "khteh",
  "author": "Kok How, Teh <funcoolgeek@gmail.com>",
  "private": true,
  "scripts": {
    "deploy": "quasar build && push-dir --dir=dist/spa --remote=khteh-page --branch=master",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:e2e": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress open\"",
    "test:e2e:ci": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress run\""
  },
  "dependencies": {
    "@quasar/cli": "^1.3.2",
    "@quasar/extras": "^1.13.5",
    "core-js": "^3.22.0",
    "firebase": "^9.6.11",
    "quasar": "^2.6.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@quasar/app-webpack": "^3.5.1",
    "@quasar/quasar-app-extension-dotenv": "^1.1.0",
    "@quasar/quasar-app-extension-testing": "^2.0.3",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^4.1.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.10",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-vue": "^8.6.0",
    "eslint-webpack-plugin": "^3.1.1",
    "push-dir": "^0.4.1"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
